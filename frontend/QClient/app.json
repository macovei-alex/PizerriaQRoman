{
  "expo": {
    "name": "PizzeriaQ",
    "slug": "pizzeriaq",
    "scheme": "pizzeriaq",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/menu-background.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.pizzeriaq"
    },
    "web": {
      "favicon": "./assets/images/logo.png"
    },
    "plugins": ["expo-router"],
    "newArchEnabled": true
  }
}
